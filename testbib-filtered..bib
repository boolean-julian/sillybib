@article{boerner_challenge_2018,
	title = {On the challenge of five-hole-probe measurements at high subsonic Mach numbers in the wake of transonic turbine cascades},
	volume = {2},
	url = {https://doi.org/10.22261/JGPPS.JPRQQM},
	doi = {10.22261/JGPPS.JPRQQM},
	abstract = {Five-hole-probes are common use in turbomachinery flow investigations, even though, inserting a probe into a flow field inevitably induces perturbations to the flow which can falsify the measurement results, especially when exposed to transonic flows. The objective of the investigations presented here is to evaluate the Mach number measurements of a five-hole-probe (5HP) in the wake flow of a transonic turbine cascade at engine relevant Reynolds numbers by comparing them to the results of particle image velocimetry ({PIV}). Furthermore, {PIV} measurements were performed with inserted probe to investigate the influence of the probe on the wake flow field. Together with a sensitivity study of 5HP measurements in flow regimes close to Ma = 1, the results demonstrate how the measurement uncertainty can be improved in high subsonic flow regimes.},
	pages = {453--464},
	journaltitle = {J. Glob. Power Propuls. Soc.},
	shortjournal = {J. Glob. Power Propuls. Soc.},
	author = {Boerner, M. and Bitter, M. and Niehuis, R. },
	date = {2018},
	file = {Boerner-OntheChallengeofFive-HoleProbeMeasurementsatHighSubsonicMachNumbersintheWakeofTransonicTurbineCascadse.pdf:G\:\\03-Mitarbeiter\\Bachner\\Literatur\\Boerner-OntheChallengeofFive-HoleProbeMeasurementsatHighSubsonicMachNumbersintheWakeofTransonicTurbineCascadse.pdf:application/pdf},
}
