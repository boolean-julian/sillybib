@article{hodson_investigation_1993,
	title = {An Investigation of Factors Influencing the Calibration of Five-Hole Probes for Three-Dimensional Flow Measurements},
	volume = {115},
	abstract = {The effects of Reynolds number, Mach number and turbulence on the calibrations of commonly used types of 5-hole probe are discussed. The majority of the probes were calibrated at the exit from a transonic nozzle over a range of Reynolds numbers (7*10ˆ3 {\textbackslash}textless 80 *10ˆ3 based on probe tip diameter) at subsonic and transonic Mach numbers. Additional information relating to the flow structure were obtained from a large sacle low speed wind tunnel. The results confirmed the existence of two distinct Reynolds number effects. Flow separation around the probe head affects the calibrations at relatively low Reynolds numbers while changes in the detailed structure of the flwo around the sensing holes affects the calibrations even when teh probe is nulled. Compressibility is shown to have little influence upon the general behaviour of these probes in terms of Reynolds number sensitivity but turbulence can effect the reliability of probe calibrations at typical test Reynolds numbers.},
	pages = {513--519},
	number = {3},
	journaltitle = {Journal of Turbomachinery},
	author = {Hodson, H. P. and Dominy, R. G. },
	date = {1993},
	keywords = {probe calibration, flwo separation, Mach number effects, probe geometry, Reynolds number effects, turbulence effects},
	file = {Dominy-AnInvestigationofFactorsInfluencingtheCalibrationofFiveHoleProbesforThreeDimensionalFlowMeasurements.pdf:G\:\\03-Mitarbeiter\\Bachner\\Literatur\\Sondenkalibrierung\\5HP\\Dominy-AnInvestigationofFactorsInfluencingtheCalibrationofFiveHoleProbesforThreeDimensionalFlowMeasurements.pdf:application/pdf},
